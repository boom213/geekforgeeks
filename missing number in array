#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int main() {
	int T;
	cin>>T;
	while(T--){
	    int n,i;
	    cin>>n;
	    int a[n];
	    int sum=0;
	    for(i=0;i<n-1;i++){
	        cin>>a[i];
	        sum+=a[i];
	    }
	    int total=(n*(n+1))/2;
	    cout<<total-sum<<endl;
	}
	return 0;
}
