#include <iostream>
using namespace std;
int main(){
    // Your code here!
    int n;
    cin>>n;
    while(n--){
        string s;
        cin>>s;
        int n=s.length();
       
       if(n==1){

			char chy[n + 1]; 
 
			strcpy(chy, s.c_str()); 
  
			for (int i = 0; i < n; i++) 
			cout << int(chy[0]); 
  
	   }
	   else{
		   cout<<"Invalid Input"<<endl;
	   }
	   
    }
    return 0;
}
